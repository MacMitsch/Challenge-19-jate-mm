{
  "name": "challenge-19-jate-mm",
  "version": "1.0.0",
  "description": "Just another Text Editor",
  "private": "true",
  "scripts": {
    "start:dev": "concurrently \"cd server && npm run server\" \"cd client && npm run dev\"",
    "start": "npm run build && cd server && node server.js",
    "server": "cd server nodemon server.js --ignore client",
    "build": "cd client && npm run build",
    "install": "concurrently \"cd client && npm install\" \"cd server && npm install\" ",
    "client": "cd client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacMitsch/Challenge-19-jate-mm.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MacMitsch/Challenge-19-jate-mm/issues"
  },
  "homepage": "https://github.com/MacMitsch/Challenge-19-jate-mm#readme",
  "dependencies": {
    "concurrently": "^7.4.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.5.0"
  }
}
